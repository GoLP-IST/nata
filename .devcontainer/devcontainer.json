{
	"name": "natadev",
	"context": "..",
	"dockerFile": "../Dockerfile",
	"settings": {
		"terminal.integrated.shell.linux": null
	},
	"extensions": [
		"ms-python.python",
		"ms-python.vscode-pylance",
		"bungcip.better-toml",
		"eamodio.gitlens",
		"github.vscode-pull-request-github"
	],
	"remoteUser": "natadev",
	"containerEnv": {
		"PYTHONBREAKPOINT": "ipdb.set_trace"
	},
	"postCreateCommand": "poetry install && poetry install -E jupyter && poetry run pip install ipdb"
}
